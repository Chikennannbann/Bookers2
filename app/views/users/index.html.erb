<div class="container">
 <div class="row">
  <div class="col-md-3">
   <%= render 'userinfo', user: @user %>

   <%= render 'profile', user: @user.id %>

   <%= render 'books/newbook', book: @book %>

  </div>

  <div class="col-md-1"></div>

  <div class="col-md-8">
   <h2>Users</h2>
   <table class="table">
    <thead>
     <tr>
      <th>image</th>
      <th>Name</th>
      <th colspan= "3"></th>
     </tr>
    </thead>

    <tbody>
     <% @users.each do |f| %>
     <tr>
       <td><%= image_tag f.get_profile_image %></td>
       <td><%= f.name %></td>
       <td><%= link_to "Show", user_path(f.id) %></td>
     </tr>
     <% end %>
    </tbody>
   </table>
  </div>
 </div>
</div>